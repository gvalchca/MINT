month = aug,
number = {3},
pages = {825--33},
pmid = {20682260},
publisher = {Biophysical Society},
title = {{Molecular dynamics simulations of mixed acidic/zwitterionic phospholipid bilayers.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2913202\&tool=pmcentrez\&rendertype=abstract},
