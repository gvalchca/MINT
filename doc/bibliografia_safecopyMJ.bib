journal = {Nucleic Acids Research},
keywords = {Databases,Internet,Models,Molecular,Nucleic Acid,Nucleic Acid Conformation,RNA,RNA: chemistry,Sequence Analysis,Software,User-Computer Interface},
month = jan,
number = {Database issue},
pages = {D386--91},
pmid = {17921499},
title = {{RNA FRABASE version 1.0: an engine with a database to search for the three-dimensional fragments within RNA structures.}},
