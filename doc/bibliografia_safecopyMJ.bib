journal = {Biophysical journal},
keywords = {Computer Simulation,Computing Methodologies,Models, Molecular,Models, Statistical,Motion,Nucleic Acid Conformation,Nucleic Acid Denaturation,Oligonucleotides,Oligonucleotides: chemistry,Solvents,Solvents: chemistry,Stochastic Processes},
month = aug,
number = {2},
pages = {790--803},
pmid = {12885628},
title = {{Insights into nucleic acid conformational dynamics from massively parallel stochastic simulations.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1303202\&tool=pmcentrez\&rendertype=abstract},
volume = {85},
year = {2003}
}
