keywords = {Base Sequence,Drug Design,Escherichia coli,Escherichia coli: chemistry,Escherichia coli: genetics,Genomics,Models, Molecular,Molecular Sequence Data,Nucleic Acid Conformation,RNA, Bacterial,RNA, Bacterial: chemistry,RNA, Bacterial: genetics,RNA, Ribosomal,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: chemistry,RNA, Ribosomal, 16S: genetics,RNA, Ribosomal, 23S,RNA, Ribosomal, 23S: chemistry,RNA, Ribosomal, 23S: genetics,RNA, Ribosomal: chemistry,RNA, Ribosomal: genetics,RNA, Transfer,RNA, Transfer: chemistry},
month = feb,
number = {3},
pages = {340--7},
pmid = {14755570},
title = {{Structural motifs in ribosomal RNAs: implications for RNA design and genomics.}},
