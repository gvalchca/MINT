journal = {Journal of the American Chemical Society},
keywords = {Entropy,Kinetics,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: chemistry,Ribosomes,Ribosomes: chemistry,Solvents,Solvents: chemistry,Static Electricity,Thermus thermophilus,Thermus thermophilus: chemistry,Thermus thermophilus: genetics},
