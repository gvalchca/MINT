keywords = {Anti-Bacterial Agents,Anti-Bacterial Agents: pharmacology,Base Sequence,DNA Primers,Molecular Sequence Data,Mutation,Nucleic Acid Conformation,RNA, Ribosomal,RNA, Ribosomal: chemistry,RNA, Ribosomal: genetics},
month = nov,
