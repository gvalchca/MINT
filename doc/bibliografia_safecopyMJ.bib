issn = {0021-9258},
journal = {The Journal of biological chemistry},
keywords = {Amino Acid Sequence,Animals,Base Sequence,Cattle,Electrophoresis, Gel, Two-Dimensional,Mass Spectrometry,Mitochondria, Liver,Mitochondria, Liver: chemistry,Molecular Sequence Data,Molecular Weight,Nucleic Acid Conformation,Peptide Mapping,RNA, Ribosomal,RNA, Ribosomal: chemistry,Ribosomal Proteins,Ribosomal Proteins: chemistry,Ribosomes,Ribosomes: chemistry,Sequence Homology, Amino Acid},
month = nov,
