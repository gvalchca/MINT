file = {:Users/aniagorska/Documents/icm/publications/Razga.Koca.Sponer.Leontis.RNA.structure.kink-turns.hinge.MD.BiophysJ.2005.pdf:pdf},
issn = {0006-3495},
journal = {Biophysical journal},
