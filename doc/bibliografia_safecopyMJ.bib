keywords = {Apoptosis,Caspase 3,Caspase 3: physiology,Caspase 9,Caspase 9: physiology,Cell Differentiation,Cell Nucleus,Cell Nucleus: chemistry,Cell Survival,Cyclin-Dependent Kinase Inhibitor p21,Cyclin-Dependent Kinase Inhibitor p21: physiology,Cytoplasm,Cytoplasm: chemistry,Humans,Neutrophils,Neutrophils: physiology,Peptide Fragments,Peptide Fragments: physiology,Proliferating Cell Nuclear Antigen,Proliferating Cell Nuclear Antigen: analysis,Proliferating Cell Nuclear Antigen: physiology,RNA, Small Interfering,RNA, Small Interfering: genetics},
month = nov,
