journal = {Journal of Molecular Biology},
keywords = {Alkyl and Aryl Transferases,Alkyl and Aryl Transferases: metabolism,Anticodon,Anticodon: chemistry,Anticodon: genetics,Anticodon: metabolism,Bacterial,Bacterial: chemistry,Bacterial: genetics,Bacterial: metabolism,Base Sequence,Biomolecular,Escherichia coli,Escherichia coli: genetics,Hydrogen Bonding,Magnesium,Magnesium: pharmacology,Magnetic Resonance Spectroscopy,Models,Molecular,Nuclear Magnetic Resonance,Nucleic Acid Conformation,Nucleic Acid Conformation: drug effects,Nucleic Acid Denaturation,Phe,Phe: chemistry,Phe: genetics,Phe: metabolism,Protein Biosynthesis,RNA,Solutions,Thermodynamics,Transfer},
