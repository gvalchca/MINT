pages = {5982--94},
pmid = {20494981},
title = {{Structural signatures of antibiotic binding sites on the ribosome.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2952860\&tool=pmcentrez\&rendertype=abstract},
volume = {38},
year = {2010}
}
